# Copy this file to .env.sh, fill in your values, then:  source .env.sh
#
# Vision One API key
#   Create one at: Vision One console → Administration → API Keys
#   Required scope: AI Security
export V1_API_KEY="your_vision_one_api_key_here"

# Vision One XDR region
#   us  → api.xdr.trendmicro.com          (US)
#   sg  → api.sg.xdr.trendmicro.com       (Singapore)
#   eu  → api.eu.xdr.trendmicro.com       (EU)
#   au  → api.au.xdr.trendmicro.com       (Australia)
#   jp  → api.jp.xdr.trendmicro.com       (Japan)
#   in  → api.in.xdr.trendmicro.com       (India)
export V1_REGION="us"

# AWS named profile with Amazon Bedrock access
#   Must have bedrock:InvokeModel permission in us-east-2
#   Leave as "default" if using the default profile
export AWS_PROFILE="default"
